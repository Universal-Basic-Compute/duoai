{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "website/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/website/$1" }
  ],
  "env": {
    "ELEVENLABS_API_KEY": "@elevenlabs_api_key"
  }
}
